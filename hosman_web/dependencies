django-hosts {
    configuration:
    create myapp.hosts.py
    settings.py {
        INSTALLED_APPS ['django-hosts']
        MIDDLEWARE / MIDDLEWARE_CLASSES [
            'django_hosts.middleware.HostsRequestMiddleware',
            'django_hosts.middleware.HostsResponseMiddleware'
        ]
        ROOT_HOSTCONF: 'myapp.hosts'
    }
}
